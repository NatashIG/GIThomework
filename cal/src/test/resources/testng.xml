<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNG Cal Suite" parallel="methods" thread-count="2">
    <test name="Cos Cal Test">
        <groups>
            <run>
                <exclude name="cos operation"/>
            </run>
        </groups>
        <classes>
          <class name="com.epam.cdp.testng.CosCalTest"/>
        </classes>
    </test>
    <test name="Ctg Cal Test">
